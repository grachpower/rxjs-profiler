{
  "name": "rxjs-profiler",
  "version": "0.0.1",
  "description": "rxjs profiler devtools",
  "main": "index.js",
  "scripts": {
    "start:example": "ts-node ./examples/index.ts",
    "test": "npm run lint && jest",
    "build": "npm run clean:dist && npm run test && tsc ./src/**.ts --outDir lib && npm run size",
    "size": "size-limit",
    "clean:dist": "npm run rimraf -- lib",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --config webpack.examples.config.js --hot --open --watch --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grachpower/rxjs-profiler.git"
  },
  "keywords": [
    "rxjs",
    "profiler",
    "devtools"
  ],
  "author": "Artyom Andreev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/grachpower/rxjs-profiler/issues"
  },
  "homepage": "https://github.com/grachpower/rxjs-profiler#readme",
  "dependencies": {
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "jest": "^23.4.1",
    "ts-lint": "^4.5.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "@types/jest": "^23.1.6",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "size-limit": "^0.18.5",
    "ts-loader": "^5.3.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "size-limit": [
    {
      "path": "lib/*.js",
      "limit": "9 KB"
    }
  ]
}
